    "*.css": [
      "stylelint --fix --allow-empty-input"
    ],
    "*.{json,html}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
